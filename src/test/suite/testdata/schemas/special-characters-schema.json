{
  "firstProperty": [ "task" ],
  "properties": {
    "task": {
      "pattern": "^SpecialCharactersTask@0$",
      "description": "friendly name\n\ndescription details",
      "ignoreCase": "value"
    },
    "inputs": {
      "properties": {
        "input name": {
          "description": "my input label",
          "type": "string",
          "ignoreCase": "key"
        }
      },
      "description": "friendly name inputs",
      "additionalProperties": false,
      "required": ["input name"]
    }
  },
  "required": ["task", "inputs"]
}